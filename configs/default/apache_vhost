
<VirtualHost *:{{PORT}}>
Include /etc/apache2/sites/_default.conf
</VirtualHost>

<VirtualHost *:{{PORT_SSL}}>
Include /etc/apache2/sites/_default.conf

SSLCertificateFile /var/www/_ssl/cert.pem
SSLCertificateKeyFile /var/www/_ssl/privkey.pem
Include /etc/letsencrypt/options-ssl-apache.conf
</VirtualHost>

